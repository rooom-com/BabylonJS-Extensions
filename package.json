{
  "name": "@rooom/babylon-htmlmesh",
  "version": "1.2.3",
  "description": "A custom version of BabylonJS HtmlMesh",
  "type": "module",
  "main": "dist/umd/babylon-htmlmesh.js",
  "module": "dist/es/babylon-htmlmesh.js",
  "exports": {
    ".": {
      "import": "./dist/es/babylon-htmlmesh.js",
      "require": "./dist/umd/babylon-htmlmesh.js",
      "types": "./dist/babylon-htmlmesh.d.ts"
    }
  },
  "files": [
    "dist/es/babylon-htmlmesh.js",
    "dist/umd/babylon-htmlmesh.js",
    "dist/babylon-htmlmesh.d.ts"
  ],
  "types": "dist/babylon-htmlmesh.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "sync-upstream": "node scripts/sync-upstream.cjs"
  },
  "author": "rooom AG <developer@rooom.com>",
  "license": "UNLICENSED",
  "private": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rooom-com/BabylonJS-Extensions.git"
  },
  "peerDependencies": {
    "@babylonjs/core": "7.x || 8.x"
  },
  "devDependencies": {
    "@babylonjs/core": "^8.13.0",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-plugin-dts": "^3.8.2"
  }
}
